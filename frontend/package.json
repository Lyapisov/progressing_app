{
  "name": "web-workers-schedule",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate-config && ng serve --host 0.0.0.0 --poll=2000",
    "build": "npm run generate-config && node --max_old_space_size=4096 node_modules/.bin/ng build --output-path=dist --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier-tslint fix src/app/**/*.ts",
    "generate-config": "./generate-config.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "~12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/language-service": "~12.2.2",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~16.7.1",
    "codelyzer": "^0.0.28",
    "jasmine-core": "^3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.3.2",
    "prettier-tslint": "^0.2.0",
    "protractor": "^7.0.0",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  }
